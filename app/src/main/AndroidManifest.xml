<?xml version="1.0" encoding="utf-8"?><!-- suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.github.smartCity">

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@drawable/google_travel_logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:fullBackupContent">
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.mytrips.TripImageActivity"
            android:configChanges="orientation|screenSize"
            android:theme="@style/AppTheme" />
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppThemeNoBar"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:host="project-travel-mate.github.io" />
                <data android:scheme="https" />
                <data android:scheme="http" />
                <data android:pathPattern=".*" />
            </intent-filter>
        </activity>

        <service
            android:name="utils.GPSTracker"
            android:enabled="true" />

        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.utilities.ChecklistActivity"
            android:label="@string/title_activity_select_city"
            android:theme="@style/AppTheme_picker" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDay6Qbk3MQD4Z80MH_HnR36TJGTrd2Ddc" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.travel.ShoppingCurrentCityActivity"
            android:label="@string/title_activity_shopping_currentcity"
            android:theme="@style/AppTheme_picker" />
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.travel.HotelsActivity"
            android:label="@string/title_activity_hotels"
            android:theme="@style/AppTheme_picker" />
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.travel.MapViewRealTimeActivity"
            android:label="@string/title_activity_map"
            android:theme="@style/AppTheme_picker" />
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppThemeNoBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.shortcuts"
                android:resource="@xml/shortcuts" />
        </activity>
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.destinations.funfacts.FunFactsActivity"
            android:label="@string/title_activity_fun_facts"
            android:theme="@style/AppTheme_picker" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.utilities.ShareContactActivity" />
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.login.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppThemeNoBar" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.destinations.description.FinalCityInfoActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.destinations.description.PlacesOnMapActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.mytrips.AddNewTripActivity" />
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.mytrips.MyTripInfoActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.destinations.description.TweetsActivity" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="io.github.smartCity.shareFile"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <activity android:name="io.github.smartCity.io.traveller.smartCity.ProfileActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.friend.FriendsProfileActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.notifications.NotificationsActivity" />
        <activity
            android:name="io.github.smartCity.io.traveller.smartCity.FullScreenImage"
            android:theme="@style/FullProfileImageTheme" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.utilities.WeatherForecastActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.destinations.description.WeatherActivity" />
    
       
        <activity android:name="io.github.smartCity.io.traveller.smartCity.destinations.description.CityHistoryActivity"/>
        <activity android:name="io.github.smartCity.io.traveller.smartCity.travel.ListViewRealTimeActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.destinations.description.RestaurantsActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.destinations.description.TweetsDescriptionActivity" />
        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity" />
        <activity android:name="io.github.smartCity.io.traveller.smartCity.DeleteProfilePictureActivity"
            android:icon="@drawable/ic_delete_round"
            android:label="@string/remove_photo"
            android:theme="@android:style/Theme.NoDisplay">
            <intent-filter>
                <action android:name="android.intent.action.DELETE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>